{
  "diskSelector": [
    {
      "name": "carina-vg-ssd",
      "re": ["loop2+"],
      "policy": "LVM",
      "nodeLabel": "kubernetes.io/hostname"
    },
    {
      "name": "carina-vg-hdd",
      "re": ["loop3+"],
      "policy": "LVM",
      "nodeLabel": "kubernetes.io/hostname"
    }
  ],
  "diskScanInterval": "300",
  "schedulerStrategy": "spreadout"
}